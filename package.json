{
  "name": "react-lightsaber",
  "version": "0.1.5",
  "description": "",
  "keywords": [],
  "main": "dist/index.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "Arnaud Zheng <arnaud.zg@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arnaud-zg/react-lightsaber.git"
  },
  "license": "MIT",
  "scripts": {
    "build:library": "rollup -c rollup.config.ts",
    "build:storybook": "build-storybook -c .storybook -o docs",
    "build:style": "gulp",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "precommit": "lint-staged",
    "release": "standard-version",
    "test:coverage:report": "npm run test:coverage && cat ./coverage/lcov.info | codacy-coverage",
    "test:coverage": "npm run test -- --coverage",
    "test": "jest --env=jsdom --config jest.config.json",
    "watch:all": "npm-run-all --parallel watch:library watch:storybook watch:style",
    "watch:library": "rollup -c rollup.config.ts -w",
    "watch:storybook": "start-storybook -p 6006",
    "watch:style": "gulp watch"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$"
  },
  "devDependencies": {
    "@storybook/react": "5.1.9",
    "@types/enzyme": "3.10.3",
    "@types/enzyme-adapter-react-16": "1.0.5",
    "@types/jest": "24.0.15",
    "@types/node": "10.14.13",
    "@types/react": "16.8.23",
    "@types/react-test-renderer": "16.8.3",
    "@types/rollup-plugin-commonjs": "9.2.0",
    "@types/rollup-plugin-json": "3.0.2",
    "@types/rollup-plugin-node-resolve": "4.0.0",
    "@types/rollup-plugin-sourcemaps": "0.4.2",
    "@types/storybook__react": "4.0.2",
    "babel-loader": "8.0.6",
    "codacy-coverage": "3.4.0",
    "colors": "1.3.3",
    "cross-env": "5.2.0",
    "css-loader": "3.1.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "gulp": "4.0.2",
    "gulp-clean-css": "4.2.0",
    "gulp-concat": "2.6.1",
    "gulp-sass": "4.0.2",
    "gulp-size": "3.0.0",
    "gulp-sizereport": "1.2.1",
    "husky": "3.0.2",
    "jest": "24.8.0",
    "jsdom": "15.1.1",
    "lint-staged": "9.2.1",
    "node-sass": "4.12.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "react-test-renderer": "16.8.6",
    "rimraf": "2.6.3",
    "rollup": "1.17.0",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-cleanup": "3.1.1",
    "rollup-plugin-commonjs": "10.0.1",
    "rollup-plugin-filesize": "6.1.1",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-peer-deps-external": "2.2.0",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rollup-plugin-typescript2": "0.22.0",
    "rollup-plugin-uglify": "6.0.2",
    "sass-loader": "7.1.0",
    "standard-version": "6.0.1",
    "style-loader": "0.23.1",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-react": "4.0.0",
    "typescript": "3.5.3"
  },
  "dependencies": {
    "react": "16.8.6",
    "react-dom": "16.8.6"
  }
}
