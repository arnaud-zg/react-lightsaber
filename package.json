{
  "name": "react-lightsaber",
  "version": "0.1.37",
  "description": "",
  "keywords": [],
  "main": "dist/index.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arnaud-zg/react-lightsaber.git"
  },
  "license": "MIT",
  "scripts": {
    "build:library": "rollup -c rollup.config.ts",
    "build:storybook": "build-storybook -c .storybook -o docs",
    "build:style": "gulp",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "prerelease": "yarn run build:storybook && git add docs/",
    "release": "standard-version -a",
    "test:coverage:report": "yarn run test:coverage && cat ./coverage/lcov.info | codacy-coverage",
    "test:coverage": "yarn run test -- --coverage",
    "test": "jest --env=jsdom --config jest.config.json",
    "watch:all": "npm-run-all --parallel watch:library watch:storybook watch:style",
    "watch:library": "rollup -c rollup.config.ts -w",
    "watch:storybook": "start-storybook -p 6006",
    "watch:style": "gulp watch"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$"
  },
  "devDependencies": {
    "@storybook/react": "5.3.13",
    "@types/enzyme": "3.10.5",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/jest": "25.1.2",
    "@types/node": "10.17.16",
    "@types/react": "16.9.21",
    "@types/react-test-renderer": "16.9.2",
    "@types/rollup-plugin-commonjs": "9.2.0",
    "@types/rollup-plugin-json": "3.0.2",
    "@types/rollup-plugin-node-resolve": "4.0.0",
    "@types/rollup-plugin-sourcemaps": "0.4.2",
    "@types/storybook__react": "4.0.2",
    "babel-loader": "8.0.6",
    "codacy-coverage": "3.4.0",
    "colors": "1.4.0",
    "cross-env": "7.0.0",
    "css-loader": "3.4.2",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "gulp": "4.0.2",
    "gulp-clean-css": "4.2.0",
    "gulp-concat": "2.6.1",
    "gulp-sass": "4.0.2",
    "gulp-size": "3.0.0",
    "gulp-sizereport": "1.2.1",
    "husky": "4.2.3",
    "jest": "25.1.0",
    "jsdom": "16.2.0",
    "lint-staged": "10.0.7",
    "node-sass": "4.13.1",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "react-test-renderer": "16.12.0",
    "rimraf": "3.0.2",
    "rollup": "1.31.1",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-cleanup": "3.1.1",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-filesize": "6.2.1",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-peer-deps-external": "2.2.2",
    "rollup-plugin-sourcemaps": "0.5.0",
    "rollup-plugin-typescript2": "0.26.0",
    "rollup-plugin-uglify": "6.0.4",
    "sass-loader": "8.0.2",
    "standard-version": "7.1.0",
    "style-loader": "1.1.3",
    "ts-jest": "25.2.0",
    "ts-node": "8.6.2",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-react": "4.1.0",
    "typescript": "3.8.2"
  },
  "dependencies": {
    "react": "16.12.0",
    "react-dom": "16.12.0"
  }
}
