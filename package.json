{
  "name": "react-lightsaber",
  "version": "0.0.5",
  "description": "",
  "keywords": [],
  "main": "dist/index.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "Arnaud Zheng <arnaud.zg@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arnaud-zg/react-lightsaber.git"
  },
  "license": "MIT",
  "scripts": {
    "build:library": "rollup -c rollup.config.ts",
    "build:storybook": "build-storybook -c .storybook -o docs",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "precommit": "lint-staged",
    "prepush": "npm run test && npm run build:library",
    "release": "standard-version",
    "test:coverage:report": "npm run test:coverage && cat ./coverage/lcov.info | codacy-coverage",
    "test:coverage": "npm run test -- --coverage",
    "test": "jest --env=jsdom --config jest.config.json",
    "watch:library": "rollup -c rollup.config.ts -w",
    "watch:storybook": "start-storybook -p 6006"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$"
  },
  "devDependencies": {
    "@storybook/react": "5.0.6",
    "@types/enzyme": "3.9.1",
    "@types/enzyme-adapter-react-16": "1.0.5",
    "@types/jest": "24.0.11",
    "@types/node": "10.14.4",
    "@types/react": "16.8.13",
    "@types/react-test-renderer": "16.8.1",
    "@types/rollup-plugin-commonjs": "9.2.0",
    "@types/rollup-plugin-json": "3.0.2",
    "@types/rollup-plugin-node-resolve": "4.0.0",
    "@types/rollup-plugin-sourcemaps": "0.4.1",
    "@types/storybook__react": "4.0.1",
    "babel-loader": "8.0.5",
    "codacy-coverage": "3.4.0",
    "colors": "1.3.3",
    "cross-env": "5.2.0",
    "css-loader": "2.1.1",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.12.1",
    "husky": "1.3.1",
    "jest": "24.7.1",
    "jsdom": "14.0.0",
    "lint-staged": "8.1.5",
    "node-sass": "4.11.0",
    "prettier": "1.17.0",
    "react-test-renderer": "16.8.6",
    "rimraf": "2.6.3",
    "rollup": "1.9.0",
    "rollup-plugin-commonjs": "9.3.0",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "4.2.1",
    "rollup-plugin-postcss": "2.0.3",
    "rollup-plugin-typescript2": "0.20.1",
    "rollup-plugin-uglify": "6.0.2",
    "sass-loader": "7.1.0",
    "standard-version": "5.0.2",
    "style-loader": "0.23.1",
    "ts-jest": "24.0.2",
    "ts-node": "8.0.3",
    "tslint": "5.15.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-react": "4.0.0",
    "typescript": "3.4.3"
  },
  "dependencies": {
    "react": "16.8.6",
    "react-dom": "16.8.6"
  }
}
